<IfModule mod_fastcgi.c>
  AddHandler php56-fcgi-{{ item }} .php
  Action php56-fcgi-{{ item }} /php56-fcgi-{{ item }}
  Alias /php56-fcgi-{{ item }} /usr/lib/cgi-bin/php56-fcgi-{{ item }}
  FastCgiExternalServer /usr/lib/cgi-bin/php56-fcgi-{{ item }} -socket /run/php/php5.6-fpm.sock -pass-header Authorization

  <Directory "/usr/lib/cgi-bin">
     Require all granted
  </Directory>
</IfModule>